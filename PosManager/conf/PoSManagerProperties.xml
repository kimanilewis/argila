    <!-- New payment status statuses on overall status-->
    <entry key="BillQuerySuccess">308</entry>

    <entry key="DaysBeforeBillDueDate">5</entry>
    <entry key="DaysAfterBillDueDate">5</entry>
    <!--limit in days in days-->
    <entry key="RefreshDaysInterval">1</entry>

    <!-- This date is cummulative of days after due date-->
    <entry key="DaysAfterActiveBillDueDate">15</entry>

    <!-- -->
    <entry key="DaysAfterLastFetch">5</entry>

    <!--Days valid to raise an overdue invoice -->
