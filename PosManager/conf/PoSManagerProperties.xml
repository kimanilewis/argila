    <entry key="RaiseInvoiceMinAmount">100</entry>
    
    <!-- This date to add on the due date to set expiry date-->
    <entry key="ExpiryDaysAfterDueDate">5</entry>
    <entry key="ConnectionRetrySleep">1000</entry>
