<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- Log configs (use full paths to the log files to specify location) -->
    <entry key="InfoLogLevel">INFO</entry>
    <entry key="ErrorLogLevel">ERROR</entry>
    <entry key="InfoLogFile">/var/log/applications/Argila/PoSManager/Info.log</entry>
    <entry key="ErrorLogFile">/var/log/applications/Argila/PoSManager/Error.log</entry>
    <!-- Application configs -->
    <entry key="NumberOfThreads">1</entry>
    <entry key="QueryLimit">1</entry>
    <entry key="ConnectionTimeout">30000</entry>
    <entry key="ReplyTimeout">90000</entry>
    <entry key="SleepTime">10</entry>
    <entry key="ApplicationName">PosManager</entry>
    <entry key="MaximumFailedQueryRetries">5</entry>
    <entry key="FAILED_QUERY_FILE">FAILED_QUERIES.TXT</entry>


    <!-- Database connection configs -->
    <entry key="DbPoolName">PosManager</entry>
