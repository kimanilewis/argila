    <entry key="NotifyCustomer">TRUE</entry>

    <!--Beep Credentials-->
    <entry key="BeepUsername">cellulantApiUser</entry>
    <entry key="BeepPassword">cc17b946d69dfb1c0931eee6383566c7</entry>

